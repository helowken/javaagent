[
  {
    "contextPath": "/test",
    "excludePackages": [
      "org\\..*"
    ],
    "transformConfigs": [
      {
        "desc": "Used to measure time cost.",
        "transformers": [
          {
            "ref": "@traceMethod"
          }
        ],
        "targets": [
          {
            "includeClasses": [
              ".*\\.TestObject"
            ]
          },
          {
            "includeClasses": [
              ".*\\.SubTestObject"
            ],
            "methodFilter": {
              "includes": [
                ".*[l|L]oad"
              ]
            }
          },
          {
            "targetClasses": [
              "org.apache.commons.lang3.StringUtils"
            ],
            "methodFilter": {
              "includes": [
                "trim"
              ]
            }
          }
        ]
      }
    ]
  }
]
