[
  {
    "contextPath": "/test",
    "excludePackages": [
      "org\\..*"
    ],
    "transformConfigs": [
      {
        "transformers": [
          {
            "ref": "@traceMethod"
          }
        ],
        "targets": [
          {
            "classFilter": {
              "includes": [
                ".*\\.TestObject"
              ]
            }
          },
          {
            "classFilter": {
              "includes": [
                ".*\\.SubTestObject"
              ]
            },
            "methodFilter": {
              "includes": [
                ".*[l|L]oad"
              ]
            }
          },
          {
            "classFilter": {
              "classes": [
                "org.apache.commons.lang3.StringUtils"
              ]
            },
            "methodFilter": {
              "includes": [
                "trim"
              ]
            }
          }
        ]
      }
    ]
  }
]
