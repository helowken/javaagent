[
  {
    "contextPath": "/test",
    "excludePackages": ["org\\..*"],
    "transformConfigs": [
      {
        "desc": "Used to measure time cost.",
        "transformers": [
          {
            "ref": "@costTimeMeasure",
            "config": {
              "log": {
                "autoFlush": false,
                "outputFormat": "[${currTime}] - ${currTimeMillis} - ${invoke}: ${costTime}ms",
                "timeFormat": "yyyy-MM-dd HH:mm:ss.SSS",
                "maxBufferSize": 8192
              }
            }
          }
        ],
        "targets": [
          {
            "targetClass": "test.jetty.TestObject",
            "methodFilter": {
              "includes": [
                "test.*"
              ]
            }
          },
          {
            "targetClass": "test.jetty.SubTestObject",
            "methodFilter": {
              "includes": [
                "load"
              ]
            }
          },
          {
            "targetClass": "org.apache.commons.lang3.StringUtils",
            "methodFilter": {
              "includes": [
                "trim"
              ]
            }
          }
        ]
      }
    ]
  },
  {
    "contextPath": "/test2",
    "transformConfigs": [
      {
        "desc": "Used to measure time cost.",
        "transformers": [
          {
            "ref": "@costTimeMeasure"
          }
        ],
        "targets": [
          {
            "targetClass": "test.jetty.TestObject",
            "methodFilter": {
              "includes": [
                "test.*"
              ]
            }
          }
        ]
      }
    ]
  }
]
