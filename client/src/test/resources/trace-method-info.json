[
    {
        "contextPath": "/test",
        "transformConfigs": [
            {
                "desc": "Used to trace method info",
                "transformers": [
                    {
                        "ref": "sys_traceMethodInfo",
                        "config": {
                            "log": {
                                "outputFormat": "${class}#${method}: \\n${content}"
                            },
                            "printerClass": ""
                        }
                    }
                ],
                "targets": [
                    {
                        "class": "test.jetty.TestObject",
                        "methodFilter": {}
                    }
                ]
            }
        ]
    }
]
