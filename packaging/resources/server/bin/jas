#!/bin/bash

export type=server
source $(dirname $0)/../../common/bin/env

server_launcher_jar=$lib_dir/agent/server-launcher-$version.jar
native_jar=$lib_dir/core/native-$version.jar
tools_jar=$JAVA_HOME/lib/tools.jar

if [ ! -f $tools_jar ]
then
    echo $tools_jar not exists.
    exit 1
fi

classpath=$common_lib_base_dir/*:$launcher_dir/*:$native_jar:$tools_jar

$JAVA_HOME/bin/java -cp $classpath \
	agent.dynamic.attach.AgentLoader \
	$server_launcher_jar=$conf_file \
	"$@"

